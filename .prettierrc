{ 
    "printWidth": 90,
    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": true,
    "semi": true,  
    "arrowParens": "always",
    "trailingComma": "es5",
    "insertPragma": false,
    "requirePragma": false,

    "overrides": [
        {
            "files": [".prettierrc", "eslintrc.json"],
            "options": { "parser": "json" }
        }
    ]
}
